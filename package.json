{
  "author": "planttheidea",
  "ava": {
    "concurrency": 5,
    "babel": "inherit",
    "failFast": true,
    "files": [
      "test/classes/*.js",
      "test/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/highcharts-config/issues"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "pathington": "^1.1.6",
    "unchanged": "^1.5.2"
  },
  "description": "Declarative Highcharts configuration builder with an immutable, chainable API",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "eslint": "^5.12.0",
    "eslint-config-rapid7": "^3.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "highcharts": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "in-publish": "^2.0.0",
    "lodash-webpack-plugin": "^0.10.7",
    "nyc": "^13.1.0",
    "optimize-js-plugin": "^0.0.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "remeasure": "^3.1.1",
    "sinon": "^7.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14"
  },
  "homepage": "https://github.com/planttheidea/highcharts-config#readme",
  "keywords": [
    "highcharts"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "highcharts-config",
  "peerDependencies": {
    "highcharts": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/highcharts-config.git"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --colors",
    "build:minified": "NODE_ENV=production webpack --progress --colors --config=webpack.config.minified.js",
    "dev": "NODE_ENV=development webpack-dev-server --progress --colors --config=webpack.config.dev.js",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run prepublish:compile || echo ''",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run transpile && npm run build && npm run build:minified",
    "start": "npm run dev",
    "test": "NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:watch": "NODE_ENV=test ava --watch",
    "transpile": "babel src --out-dir lib"
  },
  "version": "1.0.0"
}
